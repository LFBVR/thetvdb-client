{
  "env": {
    "es6": true,
    "jest/globals": true
  },
  "plugins": [
    "jest"
  ],
  "extends": [
    "airbnb-base",
    "plugin:jest/recommended"
  ],
  "parser": "babel-eslint",
  "rules": {
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxEOF": 1
      }
    ],
    "no-underscore-dangle": 0,
    "quotes": [
      "error",
      "single",
      {
        "avoidEscape": true
      }
    ]
  }
}
